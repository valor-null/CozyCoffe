{
  "expo": {
    "name": "cozy-coffe",
    "slug": "cozy-coffe",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "cozycoffe",
    "icon": "./assets/images/splash.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
